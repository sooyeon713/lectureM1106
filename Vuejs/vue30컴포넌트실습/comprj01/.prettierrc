{
  "printWidth": 80,
  "tabWidth": 2,
  "singleQuote": true,
  "semi": true,
  "useTabs": false,
  "trailingComma": "all",
  "proseWrap": "always",
  "requireConfig": false,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "endOfLine": "crlf"
}
